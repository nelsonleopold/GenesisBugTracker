@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"] - The Bug Tracker</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="keywords" content="bootstrap 4, premium, marketing, multipurpose">
    <meta name="author" content="Coderthemes">
    <!-- favicon -->
    <link rel="shortcut icon" href="~/img/favicon.ico">
    <!-- css -->
    <link href="~/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/css/style.css" rel="stylesheet" type="text/css">
</head>
<body>
    <!-- NAVBAR START -->
    <nav class="navbar navbar-expand-lg defaultscroll navbar-sticky navbar-light border-0 shadow-none" style="z-index: 1000;">
        <div class="container">
            <!-- logo -->
            <a href="index.html" class="logo">
                <img src="~img/logo.png" alt="" class="logo-light" height="22">
                <img src="~/img/logo.png" alt="" class="logo-dark" height="22">
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fe-menu"></i>
            </button>
            <!-- menus -->
            <div class="collapse navbar-collapse" id="navbarNavDropdown">
                <!-- right menu -->
                <ul class="navbar-nav ml-auto align-items-center">
                    <li class="nav-item">
                        <a class="nav-link active" href="index.html">Preview</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" target="_blank" href="docs/index.html">Documentation</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" target="_blank" href="https://themeforest.net/item/adminto-responsive-admin-dashboard/15025393/support">Support</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" target="_blank" href="https://coderthemes.com/#contact">Hire Us</a>
                    </li>
                    <li class="nav-item">
                        <div class="nav-link">
                            <a asp-page="/Account/Login" asp-area="Identity" class="btn btn-danger"><i class="fe-shopping-bag mr-1"></i> Try it Now</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- NAVBAR END -->
    <!-- START HERO -->
    <section class="position-relative overflow-hidden m-t-80">
        <div class="overlay-1"></div>
        <div class="container pt-10 pb-6">
            <div class="row align-items-center">
                <div class="col-md-5">
                    <div class="pr-lg-5">
                        <div class="badge badge-primary f-14">Version 5.2.0</div>
                        <h2 class="mb-4 mt-3">
                            The Most Complete Responsive Web UI Kit &amp; Dashboard Template
                        </h2>
                        <p class="mb-4 pt-2 f-16">
                            Adminto is a fully featured dashboard and admin template comes with tones of well designed UI elements,
                            components, widgets and applications and secondary pages.
                        </p>
                        <a asp-page="/Account/Login" asp-area="Identity" class="btn btn-primary btn-lg"><i class="fe-eye mr-1"></i> Try it now <i class="fe-arrow-right ml-1"></i></a>
                    </div>
                </div>
                <div class="col-md-7">
                    <img src="~/img/demo/main.png" class="img-fluid shadow-lg animate">
                </div>
            </div>
        </div>
    </section>
    <!-- END HERO -->
    <!-- START FEATURES -->
    <section class="section pb-6" id="demos">
        <div class="container pb-2">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-center">
                        <h3>Multiple Demos with flexible layouts</h3>
                        <p class="text-muted mt-3 mb-5">
                            There are six different demos available now with multiple layout options to <br> cater the needs
                            of any modern web application.
                        </p>
                        <h5 class="mb-0">Light Version</h5>
                        <p class="f-24"><i class="fe-minus text-primary"></i><i class="fe-git-commit text-primary"></i><i class="fe-minus text-primary"></i></p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <img src="~/img/demo/layout-1.png" alt="demo-img" class="img-fluid shadow-lg rounded">
                        <div class="demo-overlay">
                            <div class="demo-btn">
                                <a href="layouts/index.html" target="_blank" class="btn btn-light">LTR</a>
                                <a href="layouts/rtl.html" target="_blank" class="btn btn-light">RTL</a>
                            </div>
                        </div>
                    </div>
                    <h5 class="my-3 f-17 text-center">Default <small class="text-muted">(Vertical)</small></h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <img src="~/img/demo/layout-2.png" alt="demo-img" class="img-fluid shadow-lg rounded">
                        <div class="demo-overlay">
                            <div class="demo-btn">
                                <a href="layouts/layouts-horizontal.html" target="_blank" class="btn btn-light">LTR</a>
                                <a href="layouts/layouts-horizontal-rtl.html" target="_blank" class="btn btn-light">RTL</a>
                            </div>
                        </div>
                    </div>
                    <h5 class="my-3 f-17 text-center">Creative <small class="text-muted">(Horizontal)</small></h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="landing/index.html" target="_blank">
                            <img src="~/img/demo/landing.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Landing</h5>
                </div>
            </div>
            <div class="row mt-5">
                <div class="col-lg-12">
                    <div class="text-center">
                        <h5 class="mb-0">Dark Version</h5>
                        <p class="f-24"><i class="fe-minus text-primary"></i><i class="fe-git-commit text-primary"></i><i class="fe-minus text-primary"></i></p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <img src="~/img/demo/layout-3.png" alt="demo-img" class="img-fluid shadow-lg rounded">
                        <div class="demo-overlay">
                            <div class="demo-btn">
                                <a href="layouts/dark.html" target="_blank" class="btn btn-light">LTR</a>
                                <a href="layouts/dark-rtl.html" target="_blank" class="btn btn-light">RTL</a>
                            </div>
                        </div>
                    </div>
                    <h5 class="my-3 f-17 text-center">Semi Dark (Vertical)</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <img src="~/img/demo/layout-4.png" alt="demo-img" class="img-fluid shadow-lg rounded">
                        <div class="demo-overlay">
                            <div class="demo-btn">
                                <a href="layouts/layouts-horizontal-dark.html" target="_blank" class="btn btn-light">LTR</a>
                                <a href="layouts/layouts-horizontal-dark-rtl.html" target="_blank" class="btn btn-light">RTL</a>
                            </div>
                        </div>
                    </div>
                    <h5 class="my-3 f-17 text-center">Dark (Horizontal)</h5>
                </div>
            </div>
        </div>
    </section>
    <!-- START Apps -->
    <section class="section" style="background-image: url('/img/bg/bg-15.png');">
        <div class="container pt-5 pb-6">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-center">
                        <h3>Pre-built Apps &amp; Pages</h3>
                        <p class="text-muted mb-5 mt-3">Adminto comes with next generation ui design and saves lots of time!</p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/apps-chat.html" target="_blank">
                            <img src="~/img/demo/layout-5.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Chat <span class="badge text-white f-12 bg-danger ml-1">New</span></h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/apps-calendar.html" target="_blank">
                            <img src="~/img/demo/layout-6.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Calendar</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/email-inbox.html" target="_blank">
                            <img src="~/img/demo/layout-7.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Email-Inbox <span class="badge text-white f-12 bg-danger ml-1">New</span></h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/task-kanban-board.html" target="_blank">
                            <img src="~/img/demo/layout-8.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Kanban-Board</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/task-details.html" target="_blank">
                            <img src="~/img/demo/layout-9.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Task-Details</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/apps-projects.html" target="_blank">
                            <img src="~/img/demo/layout-10.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Project</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/contacts-list.html" target="_blank">
                            <img src="~/img/demo/layout-11.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Contacts</h5>
                </div>
                <div class="col-lg-4 my-3">
                    <div class="demo-box text-center">
                        <a href="layouts/contacts-profile.html" target="_blank">
                            <img src="~/img/demo/layout-12.png" alt="demo-img" class="img-fluid shadow-lg rounded animate">
                        </a>
                    </div>
                    <h5 class="my-3 f-17 text-center">Contacts Profile</h5>
                </div>
            </div>
        </div>
    </section>
    <!-- START FAQ -->
    <section class="section" style="background-image: url('/img/bg/bg-27.png');">
        <div class="container pt-6 pb-6">
            <div class="row">
                <div class="col-md-12">
                    <div class="text-center">
                        <h2>Frequently Asked Questions</h2>
                        <p class="text-muted mt-1">
                            Here are some of the basic types of questions for our customers
                        </p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center mt-4">
                <div class="col-md-8">
                    <div id="faqContent">
                        <div class="card mb-3 shadow-none border">
                            <a data-toggle="collapse" href="#collapseOne" class="accordion" aria-expanded="true" aria-controls="collapseOne">
                                <div class="card-header border-0" id="headingOne">
                                    <h6 class="mb-0">Can I use this template for my client?</h6>
                                </div>
                            </a>
                            <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#faqContent">
                                <div class="card-body pt-1">
                                    <p class="text-muted mb-0">
                                        Yup, the marketplace license allows you to use this theme
                                        in any end products.
                                        For more information on licenses, please refere <a href="https://themeforest.net/licenses/standard" target="_blank">here</a>.
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-3 border">
                            <a data-toggle="collapse" href="#collapseTwo" class="accordion collapsed" aria-expanded="true" aria-controls="collapseTwo">
                                <div class="card-header border-0" id="headingTwo">
                                    <h6 class="mb-0">Can this theme work with WordPress?</h6>
                                </div>
                            </a>
                            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#faqContent">
                                <div class="card-body pt-1">
                                    <p class="text-muted mb-0">
                                        No. This is a HTML template. It won't directly with
                                        WordPress, though you can convert this into WordPress compatible theme
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-3 border">
                            <a data-toggle="collapse" href="#collapseThree" class="accordion collapsed" aria-expanded="true" aria-controls="collapseThree">
                                <div class="card-header border-0" id="headingThree">
                                    <h6 class="mb-0">How do I get help with the theme?</h6>
                                </div>
                            </a>
                            <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#faqContent">
                                <div class="card-body pt-1">
                                    <p class="text-muted mb-0">
                                        Use our dedicated support email (support@coderthemes.com) to send your issues or feedback. We are here to help anytime
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-3 border">
                            <a data-toggle="collapse" href="#collapseFour" class="accordion collapsed" aria-expanded="true" aria-controls="collapseFour">
                                <div class="card-header border-0" id="headingfour">
                                    <h6 class="mb-0">Will you regularly give updates of Adminto ?</h6>
                                </div>
                            </a>
                            <div id="collapseFour" class="collapse" aria-labelledby="headingfour" data-parent="#faqContent">
                                <div class="card-body pt-1">
                                    <p class="text-muted mb-0">
                                        Yes, We will update the Adminto regularly. All the
                                        future updates would be available without any cost
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- END FAQ -->
    <!-- START CTA -->
    <section class="section bg-dark section-footer-1">
        <div class="container pt-10 pb-5">
            <div class="row">
                <div class="col-md-6">
                    <img src="~/img/logo-light.png" class="logo-light" alt="" height="24">
                    <p class="text-muted mt-4">
                        Adminto makes it easier to build better websites with great speed. Save
                        hundreds of hours of design and development by using it.
                    </p>
                </div>
                <div class="col-md-3 offset-md-3">
                    <h5 class="text-white f-17">Help</h5>
                    <ul class="footer-links mb-0 mt-4">
                        <li><a href="../docs/index.html" target="_blank">Documentation</a></li>
                        <li><a target="_blank" href="https://themeforest.net/item/adminto-responsive-admin-dashboard/15025393/support">Support</a></li>
                        <li><a target="_blank" href="../docs/changelog.html">Changelog</a></li>
                    </ul>
                </div>
            </div>
            <div class="row mt-5">
                <div class="col-md-12">
                    <div class="text-center">
                        <p class="text-muted mb-0">© 2015 - <script>document.write(new Date().getFullYear())</script>2022 Adminto. Design and coded by Coderthemes</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- END CTA -->
    <!-- bundle -->
    <script src="~/js/app.min.js"></script>
</body>
</html>
